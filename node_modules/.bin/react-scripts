#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mercedesz/Desktop/coding/daily-time-tracker-winter/node_modules/.pnpm/react-scripts@3.4.3_eslint@6.8.0_react@17.0.0_typescript@3.8.3/node_modules/react-scripts/bin/node_modules:/Users/mercedesz/Desktop/coding/daily-time-tracker-winter/node_modules/.pnpm/react-scripts@3.4.3_eslint@6.8.0_react@17.0.0_typescript@3.8.3/node_modules/react-scripts/node_modules:/Users/mercedesz/Desktop/coding/daily-time-tracker-winter/node_modules/.pnpm/react-scripts@3.4.3_eslint@6.8.0_react@17.0.0_typescript@3.8.3/node_modules:/Users/mercedesz/Desktop/coding/daily-time-tracker-winter/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mercedesz/Desktop/coding/daily-time-tracker-winter/node_modules/.pnpm/react-scripts@3.4.3_eslint@6.8.0_react@17.0.0_typescript@3.8.3/node_modules/react-scripts/bin/node_modules:/Users/mercedesz/Desktop/coding/daily-time-tracker-winter/node_modules/.pnpm/react-scripts@3.4.3_eslint@6.8.0_react@17.0.0_typescript@3.8.3/node_modules/react-scripts/node_modules:/Users/mercedesz/Desktop/coding/daily-time-tracker-winter/node_modules/.pnpm/react-scripts@3.4.3_eslint@6.8.0_react@17.0.0_typescript@3.8.3/node_modules:/Users/mercedesz/Desktop/coding/daily-time-tracker-winter/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../react-scripts/bin/react-scripts.js" "$@"
else
  exec node  "$basedir/../react-scripts/bin/react-scripts.js" "$@"
fi
